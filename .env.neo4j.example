# Python version for Docker
PYTHON_VERSION=3.13

# Database Configuration - Neo4j (Local Docker)
DB_TYPE=neo4j
DB_MODE=local

# Local Docker Neo4j (uses docker-compose.neo4j.yml)
NEO4J_URL=bolt://neo4j:7687
DB_USER=neo4j
DB_PASSWORD=password

# API Configuration
PORT=8000
DEBUG=true

# Redis Configuration
REDIS_URL=redis://redis:6379

# Docker Image Tag
IMAGE_TAG=local-neo4j
